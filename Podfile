  pod 'Kanna', git: 'https://github.com/tid-kijyun/Kanna.git', branch: 'swift3.0'
  

  # Pods for Westview App

  target 'Westview AppTests' do
    inherit! :search_paths
    
    
    # Pods for testing
  end

  target 'Westview AppUITests' do
    inherit! :search_paths
    
    # Pods for testing
  end

end
